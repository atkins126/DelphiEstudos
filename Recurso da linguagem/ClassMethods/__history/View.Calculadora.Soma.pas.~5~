unit View.Calculadora.Soma;

interface

uses
  View.Calculadora.Intefaces;
  type
    TCalculadoraSomas = class(TInterfacedObject, iCalculadoraOperaçoes)
      private
      public
        function Calcular (A, B : Double): Double;
      end;

implementation

{ TCalculadoraSomas }

function TCalculadoraSomas.Calcular(A, B: Double): Double;
begin
  Result := A + B;
end;

end.
