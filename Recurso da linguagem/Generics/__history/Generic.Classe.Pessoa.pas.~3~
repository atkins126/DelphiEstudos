unit Generic.Classe.Pessoa;

interface
  type
    TPessoa = class
      private
        FNome : String;
      public
        function Nome (aValue : String): TPessoa;
      end;

implementation

{ TPessoa }

function TPessoa.Nome(aValue: String): TPessoa;
begin
  Result := Self;
  FNome := aValue;
end;
end.
